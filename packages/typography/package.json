{
  "name": "@aura-ui/typography",
  "version": "0.1.3-alpha.0",
  "description": "Aura UI typographic components",
  "keywords": [
    "Typography"
  ],
  "author": "Greg Ogun <me@gregogun.xyz>",
  "homepage": "https://github.com/above-zero/aura-ui#readme",
