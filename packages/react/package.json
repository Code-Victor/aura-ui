{
  "name": "@float-ui/react",
  "version": "0.1.2-alpha.0",
  "description": "Accessible, composable React UI components built with Stitches.js",
  "keywords": [
    "react",
    "ui",
    "design-system",
    "component-library",
    "accessible",
    "stitches",
    "radix-ui"
  ],
  "author": "Greg Ogun <gregogun97@gmail.com>",
  "homepage": "https://github.com/above-zero/float-ui#readme",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/above-zero/float-ui.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/above-zero/float-ui/issues"
  },
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "clean": "rimraf node_modules dist"
  },
  "dependencies": {
    "@float-ui/button": "^0.1.2-alpha.0",
    "@float-ui/layout": "^0.1.2-alpha.0",
    "@float-ui/spinner": "^0.1.2-alpha.0",
    "@float-ui/theme": "^0.1.2-alpha.0",
    "@float-ui/typography": "^0.1.2-alpha.0",
    "@float-ui/utils": "^0.1.2-alpha.0",
    "@float-ui/visually-hidden": "^0.1.2-alpha.0"
  },
  "devDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tsup": "^5.11.7"
  }
}
