{
  "name": "@float-ui/utils",
  "version": "0.1.2-alpha.0",
  "description": "Shared utilities, configs and types for Float UI components and hooks",
  "author": "Greg Ogun <gregogun97@gmail.com>",
  "homepage": "https://github.com/above-zero/float-ui#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/above-zero/float-ui.git",
    "directory": "packages/theme"
  },
  "bugs": {
    "url": "https://github.com/above-zero/float-ui/issues"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "clean": "rimraf node_modules dist"
  },
  "dependencies": {
    "react-polymorphic-types": "^2.0.0"
  }
}
